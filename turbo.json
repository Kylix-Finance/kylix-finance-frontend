{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NEXT_PUBLIC_FRONTEND_URL",
    "NEXT_PUBLIC_RPC_ENDPOINT",
    "WAITLIST_KEY",
    "NEXT_PUBLIC_VERCEL_ENV",
    "APP_ACCESS_PASSWORD",
    "NEXT_PUBLIC_REST_API_ENDPOINT",
    "JWT_SECRET",
    "NEXT_PUBLIC_PLATFORM_URL",
    "NEXT_PUBLIC_REST_ENDPOINT",
    "NEXT_PUBLIC_BASE_ASSET",
    "NEXT_PUBLIC_RPC_END_POINT"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "check:lint": {},
    "check:lint:fix": {},
    "check:lint:fix:strict": {},
    "check:style": {},
    "check:types": {},
    "check:types:watch": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "start:dev": {
      "cache": false,
      "persistent": true
    },
    "start:production": {
      "cache": false,
      "persistent": true
    }
  }
}

{
  "name": "landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:dev": "NEXT_PUBLIC_NODE_ENV=development next build",
    "check:format": "prettier --write  --list-different ./",
    "check:lint": "eslint ./",
    "check:lint:fix": "eslint --fix ./",
    "check:lint:fix:strict": "eslint --fix --max-warnings 0 ./",
    "check:style": "pnpm run check:format && pnpm run check:lint:fix && pnpm run check:types",
    "check:types": "npx tsc --noEmit",
    "check:types:watch": "pnpm run check:types --watch",
    "dev": "pnpm run start:dev",
    "start": "pnpm run start:production",
    "start:dev": "cross-env PORT=${PORT:-3010} next dev",
    "start:dev:watch": "pnpm run start:dev &  pnpm run check:types:watch",
    "start:production": "next start"
  },
  "dependencies": {
    "@react-three/drei": "9.105.6",
    "@react-three/fiber": "8.16.3",
    "@react-three/postprocessing": "^2.16.3",
    "@svgr/webpack": "^8.1.0",
    "@types/three": "0.160",
    "clsx": "^2.1.1",
    "framer-motion": "11.2.0",
    "framer-motion-3d": "11.2.0",
    "next": "14.2.13",
    "react": "^18",
    "react-dom": "^18",
    "react-toastify": "^10.0.5",
    "recharts": "2.13.0-alpha.4",
    "tailwind-merge": "^2.4.0",
    "three": "0.160.1",
    "usehooks-ts": "^3.1.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/shared": "workspace:*",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "eslint-config-next": "14.2.13",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typescript": "^5"
  }
}

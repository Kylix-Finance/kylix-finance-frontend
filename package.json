  "scripts": {
    "build": "turbo build --concurrency=500%",
    "build:dev": "NEXT_PUBLIC_NODE_ENV=development turbo build --concurrency=500%",
    "check:format": "prettier --write  --list-different ./",
    "check:lint": "turbo check:lint --concurrency=500%",
    "check:lint:fix": "turbo check:lint:fix --concurrency=500%",
    "check:lint:fix:strict": "turbo check:lint:fix:strict --concurrency=500%",
    "check:style": "turbo check:style --concurrency=500%",
    "check:types": "turbo check:types --concurrency=500%",
    "check:types:watch": "turbo check:types:watch --concurrency=500%",
    "cleanup": " npm run cleanup:modules && npm run cleanup:ts-cache && npm run cleanup:turbo-cache --concurrency=500%",
    "cleanup:modules": "node ./scripts/utils/nodeModulesRemover.js",
    "cleanup:ts-cache": "node ./scripts/utils/tsCacheRemover.js",
    "cleanup:turbo-cache": "node ./scripts/utils/turboCacheRemover.js",
    "cleanup:turbo-cache --concurrency=500%": "node ./scripts/utils/turboCacheRemover.js",
    "dev": "pnpm run start:dev",
    "postinstall": "pnpm run setup:app",
    "setup:app": "npx husky install",
    "start": "turbo run start:production --concurrency=500%",
    "start:dev": "turbo start:dev --concurrency=500%",
    "start:dev:web": "pnpm run dev --filter=web",
    "start:production": "turbo start:production --concurrency=500%",
    "update:packageManager": "node ./scripts/utils/updatePackageManager.js pnpm"
  },
